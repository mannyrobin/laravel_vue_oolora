{
    "name": "danthecoder/saascore",
    "type": "library",
    "description": "Laravel boilerplate code that allows for rapid development of SaaS Applications",
    "authors": [
        {
            "name": "DanTheCoder",
            "email": "contact@danthecoder.com",
            "homepage": "http://danthecoder.com"
        }
    ],
    "homepage": "http://danthecoder.com",
    "require": {
        "illuminate/support": "~5",
        "nesbot/carbon": "~1.21",
        "illuminate/database": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DanTheCoder\\SaaSCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanTheCoder\\SaaSCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanTheCoder\\SaaSCore\\SaaSCoreServiceProvider"
            ]
        }
    }
}